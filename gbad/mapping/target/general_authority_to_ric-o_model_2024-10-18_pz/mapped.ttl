  rico:authorizedBy <https://data.archives.gov.on.ca/RiC-O_1-0-1/Ontario.%20Dept.%20of%20Public%20Works/Mandate/1>;
  rico:generalDescription "The Department of Public Works was responsible for providing and maintaining the buildings and other facilities necessary to house the operations of all departments of the Provincial Government of Ontario.";
  rico:hasOrHadAgentName <https://data.archives.gov.on.ca/RiC-O_1-0-1/Ontario.%20Dept.%20of%20Public%20Works/AgentName/1>,
    <https://data.archives.gov.on.ca/RiC-O_1-0-2/Ontario.%20Dept.%20of%20Public%20Works/AgentName/2>,
