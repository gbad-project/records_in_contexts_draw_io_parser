<https://data.archives.gov.on.ca/RiC-O_1-0-1/Ontario.%20Dept.%20of%20Transport/Agent/1>
  a rico:Agent;
  rdfs:label "RiC-O_1-0-1/Ontario. Dept. of Transport/Agent/1";
  rico:authorizedBy <https://data.archives.gov.on.ca/RiC-O_1-0-1/Ontario.%20Dept.%20of%20Transport/Mandate/1>;
  rico:generalDescription "The Department of Transport was responsible for the administration of matters relating to motor vehicle traffic, including the licensing of motor vehicles, the examination of drivers, and highway safety.";
  rico:hasOrHadAgentName <https://data.archives.gov.on.ca/RiC-O_1-0-1/Ontario.%20Dept.%20of%20Transport/AgentName/1>,
    <https://data.archives.gov.on.ca/RiC-O_1-0-2/Ontario.%20Dept.%20of%20Transport/AgentName/2>,
