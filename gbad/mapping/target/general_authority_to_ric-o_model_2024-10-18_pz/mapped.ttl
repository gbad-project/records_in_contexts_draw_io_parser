<https://data.archives.gov.on.ca/RiC-O_1-0-1/Ontario.%20Ministry%20of%20Housing%20%281985-1995%29/Agent/1>
  a rico:Agent;
  rdfs:label "RiC-O_1-0-1/Ontario. Ministry of Housing (1985-1995)/Agent/1";
  rico:authorizedBy <https://data.archives.gov.on.ca/RiC-O_1-0-1/Ontario.%20Ministry%20of%20Housing%20%281985-1995%29/Mandate/1>;
  rico:generalDescription "The Ministry of Housing was responsible for programs and strategies to ensure affordable rental housing for low and moderate income households in the province. Activities of the Ministry included: providing rent-geared-to-income housing; supporting municipal, private and co-operative non-profit housing; administering rent review legislation; developing and maintaining the Ontario Building and Plumbing Codes; and developing Ontario's building industry.";
  rico:hasOrHadAgentName <https://data.archives.gov.on.ca/RiC-O_1-0-1/Ontario.%20Ministry%20of%20Housing%20%281985-1995%29/AgentName/1>,
