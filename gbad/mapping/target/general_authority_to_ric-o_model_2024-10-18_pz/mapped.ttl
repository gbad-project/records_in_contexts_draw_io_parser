  rico:hasOrHadCorporateBodyType <https://data.archives.gov.on.ca/Schema/Authority/CorporateBodyType/A%20Ontario%20Government%20Name>;
  rico:hasOrHadIdentifier <https://data.archives.gov.on.ca/RiC-O_1-0-1/Ontario.%20Ministry%20of%20Energy%2C%20Science%20and%20Technology/Identifier/1>;
